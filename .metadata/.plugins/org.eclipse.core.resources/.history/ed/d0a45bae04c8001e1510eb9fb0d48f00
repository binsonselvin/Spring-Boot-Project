package com.example4;

public class Example2 {
	
	public static void main(String[] args) {
		
		//With return value
		DemoInterface demoImpl2 = (num1, num2)-> {
			return num1+num2;
		};
		
		Runnable thread = () -> {
			System.out.println("Starting thread 2");
		};
		
		Thread myObj = new Thread(thread);
		myObj.start();
		
		System.out.println("Addition value: "+demoImpl2.addTwoNum(10,49));
	}
}
